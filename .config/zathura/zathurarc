set default-bg \#231e18 # bg
set default-fg \#cabcb1 # fg

set adjust-open "best-fit"

set font "iosevka aile normal 13"

set guioptions svh

set scrollbar-bg \#231e18 # bg
set scrollbar-fg \#cabcb1 # fg

set statusbar-bg \#231e18 # bg
set statusbar-fg \#cabcb1 # fg

set inputbar-bg \#231e18 # bg
set inputbar-fg \#b49368 # accent

set completion-bg \#231e18 # bg
set completion-fg \#cabcb1 # fg
set completion-highlight-bg \#b49368 # accent
set completion-highlight-fg \#302b25 # bg_alt

set recolor
set recolor-darkcolor \#cabcb1 # fg
set recolor-lightcolor \#231e18 # bg
set recolor-reverse-video
set recolor-keephue
